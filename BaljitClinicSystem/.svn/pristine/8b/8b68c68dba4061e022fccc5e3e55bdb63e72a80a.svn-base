    function createNew(){
        var $this = $(this);
        
        $.ajax({
            type: "POST",
            url: "${pageContext.request.contextPath}/Servlet_Supplier?SFC=INSERT_SUPPLIER",
            data: {
                ADDSUP: JSON.stringify($('#form_AddSupplier').serializeArray()),
                MODE: "ADD_SUPPLIERMASTER"
            },
            dataType: 'json',
            beforeSend: function (xhr) {
                $.showLoadingNoStatus();
            },
            success: function(response,xhr,status){
                if (response.bool === true) {
                    $.bigBoxSuccess({
                        title: "Create Supplier SUCCESS.",
                        content: "Supplier creation SUCCESS.",
                        timeout: 6000
                    });
                }
                else {
                    $.bigBoxFail({
                        title: "Create Supplier FAIL.",
                        content: "There is some error during supplier creation.<br> Please try again...",
                        timeout: 6000
                    });
                }
            },
            error: function (xhr, status, error) {
                $.bigBoxFail({
                    title: "Transaction ERROR",
                    content: "There is some error occur during data transfer. Please try again later",
                    timeout: 6000
                });
            },
            complete: function (xhr, status) {
                if (xhr.status === 200) {
                    $.hideLoadingNoStatus(function () {
                   window.location.href = "#/SupplierDetail/" + $('#txtSupCode').val();
                    }, 500);
                } else {
                    $.hideLoadingNoStatus(function () {
                    }, 0);
                }
            }
        });
    }
    //c02
    //Aaron
            
    
    pagefunction();
</script>
