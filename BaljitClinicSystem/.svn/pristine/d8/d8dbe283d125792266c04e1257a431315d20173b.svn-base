    function confirmGRN()
    {
        $.ajax({
            type: "POST",
            url: "${pageContext.request.contextPath}/Servlet_GRN?SFC=CONFIRM_GRN",
            data: {
                GRNIDX: $("#hidGRNID").val(), "CONTROL_DATETIME":  $('#ControlDateTime').val()
            },
            dataType: 'json',
            beforeSend: function () {
                $.showLoadingNoStatus();
            },
            success: function(data) {
                var arrReturn = data;                                      
                if (arrReturn.bool === true) {
                    //start PO fulfill update
                    $.ajax({
                        type: "POST",
                        url: "${pageContext.request.contextPath}/Servlet_PurchaseOrder?SFC=UPDATE_PO_FULFILL",
                        data: {
                            GRNIDX: $("#hidGRNID").val()
                        },
                        dataType: 'json',                  
                        success: function(data) {
                            var arrReturn_PO = data;                  
                            $.hideLoadingNoStatus(function () {                
                                if (arrReturn_PO.bool === true) {
                                    $.bigBoxSuccess({
                                        title: "GRN SUCCESS CONFIRMED.",
                                        content: "GRN Confirmed",
                                        timeout: 6000
                                    });
                                    showDisplay('CONFIRMED');
                                } else {
                                    $.bigBoxFail({
                                        title: "FAILED To Confirm GRN.",
                                        content: "There is some error during PO Update.<br> Please try again",
                                        timeout: 6000
                                    });
                                }
                            },500);
                        },
                        error: function() {                         
                            $.bigBoxFail({
                                title: "FAILED To Confirm GRN.",
                                content: "There is some error during PO Update.<br> Please try again",
                                timeout: 6000
                            });                          
                        },
                        complete: function (xhr, status) {
                            if (xhr.status === 200) {
                                $.hideLoadingNoStatus(function () {
                                   location.reload();
                                }, 500);
                            } else {
                                $.hideLoadingNoStatus(function () {
                                }, 0);
                            }
                        }
                    });                           
                } else {
                    $.hideLoadingNoStatus(function () {   
                        $.bigBoxFail({
                            title: "FAILED To Confirm GRN.",
                            content: "There is some error during GRN confirmation.<br> Please try again... <br><br>" + arrReturn.msg,
                            timeout: 6000
                        });
                    },500);
                }
            }
        });
    }
    
