    function calGST(gstRate, unitPrice, GSTIn, DiscType, Qty, DiscVal)
    { 
        // gstRate : 0 or 0.06        
        if (gstRate > '1') {
            gstRate = gstRate / 100;
        }
        
        var grossTotal = 0, gstAmt = 0, nettTotal = 0;
        var grossTotal_AfterGst = 0, disc;
        
            if(DiscType === 'P'){
            disc = (unitPrice * Qty) * (DiscVal / 100);          
        } else {
            disc = DiscVal;        
            }      
                grossTotal = (unitPrice * Qty) - disc;
        
        if(GSTIn === "N"){
            if (gstRate > '0') {
                gstAmt = grossTotal * gstRate;
                nettTotal = grossTotal + gstAmt;
            } else {
                nettTotal = grossTotal;
            }
        } else {
            if (gstRate > '0') {
                grossTotal_AfterGst = grossTotal / 106 * 100;
                gstAmt = grossTotal - grossTotal_AfterGst;
                nettTotal = grossTotal_AfterGst + gstAmt;
                
                grossTotal = grossTotal_AfterGst;
            } else {
                nettTotal = grossTotal;
        }
        }
        
        nettTotal = nettTotal.toFixed(2);
        grossTotal = grossTotal.toFixed(2);
        gstAmt = gstAmt.toFixed(2);
        return {gstAmt : gstAmt, grossTotal : grossTotal, nettTotal : nettTotal};        
    }
    
