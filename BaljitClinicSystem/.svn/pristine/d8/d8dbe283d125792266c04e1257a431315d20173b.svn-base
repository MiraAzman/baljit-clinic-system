    function BindStockUOM(stkCode, uom, convUOM) { 
            $.get('${pageContext.request.contextPath}/Servlet_Stock', {SFC: 'GET_REL_STKUOM', STKCODE: stkCode}, function (data) {
                myStkUOM = JSON.parse(data);

                //var ddl = $("#ddlUOM");
                createOption($("#ddlUOM"), myStkUOM.rows, '- Select UOM -');
                $("#ddlUOM").val(uom);
                
                createOption($("#ddlConvUOM"), myStkUOM.rows, '(No Conversion)'); 
                if (convUOM !== "") {
                    $("#ddlConvUOM").val(convUOM);
                }
            });
    }
    
    function getStockSupplier(stkCode, CompCode, SuppCode){
        $.get('${pageContext.request.contextPath}/Servlet_Company', {SFC: 'GET_COMP_SUPP_STK', STKCODE: stkCode, COMPCODE: CompCode, SUPPCODE: SuppCode}, function (Compdata) {                                                        
            var compStkjsonData = JSON.parse(Compdata);                                                         

            if (compStkjsonData.rows.length > 0) {
                $("#txtCost").val(parseFloat(compStkjsonData.rows[0].UnitCost).toFixed(2));
            }
        });
    }
    
    function convertUOM(){
        var ddlUOM = $("#ddlUOM").val(); 
        var ddlConvUOM = $("#ddlConvUOM").val();
                
        if (ddlConvUOM === "") {
            $("#txtConvQty, #txtConvCost").val("");
        }
        
        if(ddlConvUOM !== '' && ddlUOM !== ''){
            var qty = parseInt($("#txtRcvQty").val()) + parseInt($("#txtRcvBonus").val()); 
            
            $.get('${pageContext.request.contextPath}/Servlet_Stock', {
                SFC: 'CONVERT_UOM',   
                QTY: qty,
                CURRUOM: ddlConvUOM,
                PREVUOM: ddlUOM,
                PRODCODE: $("#txtPrdCode").val(),
                UNITCOST: $("#txtCost").val()
            }, function (data) {                 
                $("#txtConvQty").val(JSON.parse(data).rows[0].RcvQty);
                $("#txtConvCost").val(JSON.parse(data).rows[0].Cost);
            });
        }
        
        var gstRate = 
                $('#hidGST').val(),
                unitPrice = $('#txtCost').val(),
                GSTIn = $('#chkGSTInc').is(':checked') ? "Y" : "N",
                DiscType = $(":radio[name='rdDisc']:checked").val(),
                Qty = $('#txtRcvQty').val(),
                DiscVal = $('#txtDisc').val(),
                dataGST;
                
            dataGST = calGST(gstRate, unitPrice, GSTIn, DiscType, Qty, DiscVal);
            $("#txtGSTAmt").val(dataGST.gstAmt);
            $("#txtGross").val(dataGST.grossTotal);
            $("#txtNettTotal").val(dataGST.nettTotal);
        //$("#ddlUOMPrevious").val($("#ddlUOM").val());           
    }
    
